def graph_audio(audio):
  y,sr=librosa.load(audio)
  dt=y.size/sr
  plt.figure()
  plt.subplot()
  plt.grid()
  plt.xlim(0.3,dt)
  plt.ylim(-0.08,0.081)
  librosa.display.waveshow(y, sr=sr)
  plt.xlabel('tiempo [s]')
  plt.ylabel('amplitud[/]')

print("NO_SMOKERS")
for i in os.listdir("/content/Data/NO_SMOKERS"):
  graph_audio(f"/content/Data/NO_SMOKERS/{i}")

